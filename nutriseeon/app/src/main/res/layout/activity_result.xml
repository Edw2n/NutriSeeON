<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#97EDE7AC"
    tools:context=".ResultActivity">

    <TextView
        android:id="@+id/tv_otherVal"
        android:layout_width="116dp"
        android:layout_height="160dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="30dp"
        android:textAlignment="viewStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_other"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tv_otherName"
        android:layout_width="116dp"
        android:layout_height="160dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="30dp"
        android:textAlignment="viewStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tv_otherVal"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_other"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tv_nutriName"
        android:layout_width="116dp"
        android:layout_height="136dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="20dp"
        android:textAlignment="viewStart"
        app:layout_constraintEnd_toStartOf="@+id/tv_nutriVal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_important" />

    <TextView
        android:id="@+id/tv_nutriVal"
        android:layout_width="116dp"
        android:layout_height="136dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="80dp"
        android:textAlignment="viewStart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_important" />

    <TextView
        android:id="@+id/tv_other"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="146dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="146dp"
        android:text="Other nutrients"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_nutriName" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="230dp"
        android:layout_height="45dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="93dp"
        android:layout_marginEnd="90dp"
        android:text="RESULT"
        android:textAlignment="center"
        android:textSize="32sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_important"
        android:layout_width="210dp"
        android:layout_height="26dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="100dp"
        android:text="Important nutrients"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />
</android.support.constraint.ConstraintLayout>